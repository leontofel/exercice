// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  user_id    Int     @default(autoincrement()) @id
  name  String  @unique
  city  String
  pet  String
  tech_stack  String
  married  Boolean
  birthday  DateTime 
  email String  @unique
  password  String
}