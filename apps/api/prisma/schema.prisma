// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())
  name       String
  city       String
  pet        String
  tech_stack String
  married    Boolean
  birthday   DateTime
  email      String   @unique
  password   String
}
